data <- data[order(studlab,treat),]
if(type=="bin") {
event <- data$event <- eval(substitute(event), data)
dp <- pairwise(treat=data$treat, event=data$event, n=data$n, studlab=data$studlab)
tab <- matrix(as.numeric(table(dp$treat1, dp$treat2)), nrow = nrow(table(dp$treat1, dp$treat2)),
ncol=ncol(table(dp$treat1, dp$treat2)),
dimnames = list(1:length(unique(dp$treat1)),unique(dp$treat2)))       # this table stores the number of studies per each comparison
num.comp <- print(paste("There are ", sum(tab>9), "comparisons with at least 10 studies"))
results <- matrix(nrow = sum(tab>9), ncol = 3)
m <- mapply(function(i,j) if(tab[i,j]>9) {
ma <- metabin(event1,n1,event2, n2, subset = treat1==i & treat2==j+1, data = dp)
funnel(ma, contour = c(0.9, 0.95, 0.99), col.contour = c("darkgreen", "green", "lightgreen"))
legend("topright", c("0.1 > p > 0.05", "0.05 > p > 0.01", "< 0.01"),
fill = c("darkgreen", "green", "lightgreen"))
mtext(paste(i, "vs", j+1))
list(paste(i, "vs", j+1),
round(metabias(ma)$estimate["slope"], digits = 2),     # default here is score test (Harbord)
round(metabias(ma)$p.value, digits = 2))
}, 1:nrow(tab), rep(1:ncol(tab),each=nrow(tab)))            # this produces contour-enhanced funnel plots and test for small-study effect for comparisons with at least 10 studies
results <- matrix(sapply(1:length(compact(m)), function(i) results[i,] <- compact(m)[[i]]),
nrow = length(compact(m)), ncol = 3, byrow = T)
interpretation <- sapply(1:length(compact(m)), function(i)
ifelse(results[i,2]>0,"Small studies give smaller effect for 1st intervention",
"Small studies give larger effect for 1st intervention"))
results <- cbind(results, interpretation)
colnames(results) <- c("comparison", "slope", "p-value", "interpretation")
return(results)             # the function returns a matrix where the slope and p-value of the regression test for small-study effects and relevant interpretation are reported per each comparison with at least 10 studies
}
else  {
mean <- data$mean <- eval(substitute(mean), data)
sd <- data$sd <- eval(substitute(sd), data)
dp <- pairwise(treat, n, mean, sd, data, studlab)
tab <- matrix(as.numeric(table(dp$treat1, dp$treat2)), nrow = nrow(table(dp$treat1, dp$treat2)),
ncol=ncol(table(dp$treat1, dp$treat2)),
dimnames = list(1:length(unique(dp$treat1)),unique(dp$treat2)))
#results <- matrix(nrow = sum(tab>9), ncol = 3)
m <- mapply(function(i,j) if(tab[i,j]>9) {
ma <- metagen(TE,seTE,subset = treat1==i & treat2==j+1, sm=sm, data=dp)
funnel(ma, contour = c(0.9, 0.95, 0.99), col.contour = c("darkgreen", "green", "lightgreen"))
legend("topright", c("0.1 > p > 0.05", "0.05 > p > 0.01", "< 0.01"),
fill = c("darkgreen", "green", "lightgreen"))
mtext(paste(i, "vs", j+1))
list(paste(i, "vs", j+1),
round(metabias(ma)$estimate["slope"], digits = 2),     # default here is egger test (linreg)
round(metabias(ma)$p.value, digits = 2))
}, 1:nrow(tab), rep(1:ncol(tab),each=nrow(tab)))
results <- matrix(sapply(1:length(compact(m)), function(i) results[i,] <- compact(m)[[i]]),
nrow = length(compact(m)), ncol = 3, byrow = T)
interpretation <- sapply(1:length(compact(m)), function(i)
ifelse(results[i,2]>0,"Small studies give smaller effect for 1st intervention",
"Small studies give larger effect for 1st intervention"))
results <- cbind(results, interpretation)
colnames(results) <- c("comparison", "slope", "p-value", "interpretation")
return(results)
}
}
